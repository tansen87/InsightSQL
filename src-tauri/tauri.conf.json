{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080"
  },
  "bundle": {
    "active": true,
    "targets": [
      "dmg",
      "deb",
      "appimage",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "resources": [],
    "externalBin": [],
    "longDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "insight-x",
  "mainBinaryName": "insight-x",
  "version": "../package.json",
  "identifier": "com.insight-x.dev",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "width": 840,
        "height": 660,
        "resizable": true,
        "title": "insight-x",
        "dragDropEnabled": false,
        "center": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}